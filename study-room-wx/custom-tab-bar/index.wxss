.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  background: white;
  display: flex;
  padding-bottom: 15rpx;
  justify-content: space-around;
}


.tab-bar-item {
  width: 100rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
}

.tab-bar-item cover-image {
  margin: 0 auto;
  width: 60rpx;
  height: 60rpx;
}

.tab-bar-item cover-view {
  font-size: 10px;
}

.text-active {
  color: #17B3A3;
}
.footer {
  width: 100%;
  height: 204rpx;
}
